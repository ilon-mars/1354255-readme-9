{
  "name": "@project/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:user": "npx nx run user:serve",
    "start:blog": "npx nx run blog:serve",
    "start:notify": "npx nx run notify:serve",
    "account:docker-up": "docker compose --file ./apps/account/docker-compose.dev.yml --project-name readme-account --env-file ./apps/account/account.env up -d",
    "account:docker-down": "docker compose --file ./apps/account/docker-compose.dev.yml --project-name readme-account --env-file ./apps/account/account.env down",
    "blog:docker-up": "docker compose --file ./apps/blog/docker-compose.dev.yml --env-file ./apps/blog/blog.env --project-name blog-readme up -d",
    "blog:docker-down": "docker compose --file ./apps/blog/docker-compose.dev.yml --env-file ./apps/blog/blog.env --project-name blog-readme down",
    "notify:docker-up": "docker compose --file ./apps/notify/docker-compose.dev.yml --env-file ./apps/notify/notify.env --project-name readme-notify up -d",
    "notify:docker-down": "docker compose --file ./apps/notify/docker-compose.dev.yml --env-file ./apps/notify/notify.env --project-name readme-notify down",
    "storage:docker-up": "docker compose --file ./apps/storage/docker-compose.dev.yml --env-file ./apps/storage/storage.env --project-name readme-storage up -d",
    "storage:docker-down": "docker compose --file ./apps/storage/docker-compose.dev.yml --env-file ./apps/storage/storage.env --project-name readme-storage down",
    "all:docker-up": "npm run account:docker-up && npm run notify:docker-up && npm run blog:docker-up && npm run storage:docker-up",
    "all:docker-down": "npm run account:docker-down && npm run notify:docker-down && npm run blog:docker-down && npm run storage:docker-down"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 2
  },
  "private": true,
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "5.6.1",
    "@nestjs-modules/mailer": "2.0.2",
    "@nestjs/axios": "4.0.0",
    "@nestjs/common": "10.0.2",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "10.0.2",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/mongoose": "11.0.3",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "10.0.2",
    "@nestjs/swagger": "8.1.0",
    "@prisma/client": "6.8.2",
    "@types/multer": "1.4.12",
    "axios": "1.6.0",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "dayjs": "1.11.13",
    "docker-compose": "1.2.0",
    "fs-extra": "11.3.0",
    "joi": "17.13.3",
    "mongoose": "8.15.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "prisma": "6.8.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
